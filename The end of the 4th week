outer:
for(var i = 1; i <= 3; i++){
    inner:
    for (var j = 0; j <= 5; j++){
        while (j == 2){
            document.writeln("skipped");
            continue inner;
        } while (j == 4){
            document.writeln("terminated");
            break inner;
        }
        document.writeln("i : " + i + ", j :" + j);
    }
    document.writeln();
}
